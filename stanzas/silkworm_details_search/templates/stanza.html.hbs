<style>
@import url('https://rcshige3.nig.ac.jp/rdf/css/bootstrap.min.css');
</style>

<div class="table-responsive">

	<!--
		系統リソース情報
	-->
	{{#if information}}
	<table class="table table-sm table-striped table-bordered table-information">
		<thead class="thead-dark">
			<th colspan="2"><h4>Information</h4></th>
		</thead>
		<tr>
			<th class="sub-title">Strain</th>
			<td>{{information.strain}}</td>
		</tr>
		<tr>
			<th class="sub-title">Taxonomy ID</th>
			<td><a href="{{information.taxonomy_id}}" target="_blank">{{information.taxonomy_id}}</a></td>
		</tr>
		<tr>
			<th class="sub-title">Origin</th>
			<td>{{{information.origin}}}</td>
		</tr>
		<tr>
			<th class="sub-title">Description</th>
			<td>{{{information.description}}}</td>
		</tr>
		<tr>
			<th class="sub-title">Reference</th>
			<td>
				{{#each reference as |reference|}}
					{{reference.author_name}} ({{reference.date}}). {{reference.puclication_name}}, {{reference.volume}}, {{reference.number}}, {{reference.starting_page}}-{{reference.ending_page}}.
				<a href="{{reference.url}}" target="_blank">{{reference.url}}</a><br/>
				{{/each}}
			</td>
		</tr>
		<tr>
			<th class="sub-title">Journal</th>
			<td>{{{information.journal}}}</td>
		</tr>
		<tr>
			<th class="sub-title">HP</th>
			<td><a href="{{information.homepage}}" target="_blank">{{information.homepage}}</a></td>
		</tr>
	</table>
	{{else}}
	<p>No data found.</p>
	{{/if}}

	<!--
		卵リソース情報
	-->
	{{#if egg}}
	<table class="table table-sm table-striped table-bordered table-egg">
		<thead class="thead-dark">
			<th colspan="4"><h4>Egg</h4></th>
		</thead>
		<tr>
			<th colspan="1" class="sub-title">Resource State</th>
			<td colspan="3">{{egg.resource_state}}</td>
		</tr>
		<tr>
			<th colspan="1" class="sub-title">Image</th>
			<td colspan="3">
		{{#if egg_image}}
			{{#each egg_image as |egg_image|}}
				<img width="120px" src="{{egg_image.image}}">
			{{/each}}
		{{/if}}
			</td>
		</tr>
		{{#if egg_phenotype}}
		<thead class="thead-dark">
			<th colspan=4">Phenotype</th>
		</thead>
		<tr>
			<th colspan="1">Phenotype</th>
			<th colspan="1">BMPO</th>
			<th colspan="1">seeAlso</th>
			<th colspan="1">Reference</th>
		</tr>
			{{#each egg_phenotype as |egg_phenotype|}}
		<tr>
			<td colspan="1">{{{egg_phenotype.label}}}</td>
			<td colspan="1">{{{egg_phenotype.bmpo}}}</td>
			<td colspan="1">{{{egg_phenotype.related_class}}}</td>
			<td colspan="1">{{{egg_phenotype.reference}}}</td>
		</tr>
			{{/each}}
		{{/if}}
		{{#if egg_gene}}
		<thead class="thead-dark">
			<th colspan=4">Gene</th>
		</thead>
		<tr>
			<th colspan="1">Gene ID</th>
			<th colspan="1">Name</th>
			<th colspan="1">Synonym</th>
			<th colspan="1">Reference</th>
		</tr>
			{{#each egg_gene as |egg_gene|}}
		<tr>
			<td colspan="1"><a href="https://www.ncbi.nlm.nih.gov/gene/{{egg_gene.id}}" target="_blank">{{egg_gene.id}}</a></td>
			<td colspan="1">{{egg_gene.name}}</td>
			<td colspan="1">{{egg_gene.synonym}}</td>
			<td colspan="1">{{{egg_gene.url}}}</td>
		</tr>
			{{/each}}
		{{/if}}
	</table>
	{{/if}}

	<!--
		幼虫リソース情報
	-->
	{{#if larva}}
	<table class="table table-sm table-striped table-bordered table-larva">
		<thead class="thead-dark">
			<th colspan="4"><h4>Larva</h4></th>
		</thead>
		<tr>
			<th colspan="1" class="sub-title">Resource State</th>
			<td colspan="3">{{larva.resource_state}}</td>
		</tr>
		<tr>
			<th colspan="1" class="sub-title">Image</th>
			<td colspan="3">
		{{#if larva_image}}
			{{#each larva_image as |larva_image|}}
				<img width="120px" src="{{larva_image.image}}">
			{{/each}}
		{{/if}}
			</td>
		</tr>
		{{#if larva_phenotype}}
		<thead class="thead-dark">
			<th colspan="4">Phenotype</th>
		</thead>
		<tr>
			<th colspan="1">Phenotype</th>
			<th colspan="1">BMPO</th>
			<th colspan="1">seeAlso</th>
			<th colspan="1">Reference</th>
		</tr>
			{{#each larva_phenotype as |larva_phenotype|}}
		<tr>
			<td colspan="1">{{{larva_phenotype.label}}}</td>
			<td colspan="1">{{{larva_phenotype.bmpo}}}</td>
			<td colspan="1">{{{larva_phenotype.related_class}}}</td>
			<td colspan="1">{{{larva_phenotype.reference}}}</td>
		</tr>
			{{/each}}
		{{/if}}
		{{#if larva_feeding}}
		<thead class="thead-dark">
			<th colspan="4">Feeding ability of artificial diets</th>
		</thead>
		<tr>
			<th colspan="1">Feeding ability</th>
			<th colspan="1">BMPO</th>
			<th colspan="1">seeAlso</th>
			<th colspan="1">Description</th>
		</tr>
			{{#each larva_feeding as |larva_feeding|}}
		<tr>
			<td colspan="1">{{{larva_feeding.label}}}</td>
			<td colspan="1">{{{larva_feeding.bmpo}}}</td>
			<td colspan="1">{{{larva_feeding.related_class}}}</td>
			<td colspan="1">{{{larva_feeding.description}}}</td>
		</tr>
			{{/each}}
		{{/if}}
		{{#if larva_period}}
		<thead class="thead-dark">
			<th colspan="4">Larval Period</th>
		</thead>
		<tr>
			<th colspan="1">Larval Period</th>
			<th colspan="1">BMPO</th>
			<th colspan="1">seeAlso</th>
			<th colspan="1">Description</th>
		</tr>
			{{#each larva_period as |larva_period|}}
		<tr>
			<td colspan="1">{{{larva_period.label}}}</td>
			<td colspan="1">{{{larva_period.bmpo}}}</td>
			<td colspan="1">{{{larva_period.related_class}}}</td>
			<td colspan="1">{{{larva_period.description}}}</td>
		</tr>
			{{/each}}
		{{/if}}
		{{#if larva_gene}}
		<thead class="thead-dark">
			<th colspan=4">Gene</th>
		</thead>
		<tr>
			<th colspan="1">Gene ID</th>
			<th colspan="1">Name</th>
			<th colspan="1">Synonym</th>
			<th colspan="1">Reference</th>
		</tr>
			{{#each larva_gene as |larva_gene|}}
		<tr>
			<td colspan="1"><a href="https://www.ncbi.nlm.nih.gov/gene/{{larva_gene.id}}" target="_blank">{{larva_gene.id}}</a></td>
			<td colspan="1">{{larva_gene.name}}</td>
			<td colspan="1">{{larva_gene.synonym}}</td>
			<td colspan="1">{{{larva_gene.url}}}</td>
		</tr>
			{{/each}}
		{{/if}}
	</table>
	{{/if}}

	<!--
		蛹リソース情報
	-->
	{{#if pupa}}
	<table class="table table-sm table-striped table-bordered table-pupa">
		<thead class="thead-dark">
			<th colspan="4"><h4>Pupa</h4></th>
		</thead>
		<tr>
			<th colspan="1" class="sub-title">Resource State</th>
			<td colspan="3">{{pupa.resource_state}}</td>
		</tr>
		<tr>
			<th colspan="1" class="sub-title">Image</th>
			<td colspan="3">
		{{#if pupa_image}}
			{{#each pupa_image as |pupa_image|}}
				<img width="120px" src="{{pupa_image.image}}">
			{{/each}}
		{{/if}}

			</td>
		</tr>
		{{#if pupa_phenotype}}
		<thead class="thead-dark">
			<th colspan=4">Phenotype</th>
		</thead>
		<tr>
			<th colspan="1">Phenotype</th>
			<th colspan="1">BMPO</th>
			<th colspan="1">seeAlso</th>
			<th colspan="1">Reference</th>
		</tr>
			{{#each pupa_phenotype as |pupa_phenotype|}}
		<tr>
			<td colspan="1">{{{pupa_phenotype.label}}}</td>
			<td colspan="1">{{{pupa_phenotype.bmpo}}}</td>
			<td colspan="1">{{{pupa_phenotype.related_class}}}</td>
			<td colspan="1">{{{pupa_phenotype.reference}}}</td>
		</tr>
			{{/each}}
		{{/if}}
		{{#if pupa_gene}}
		<thead class="thead-dark">
			<th colspan=4">Gene</th>
		</thead>
		<tr>
			<th colspan="1">Gene ID</th>
			<th colspan="1">Name</th>
			<th colspan="1">Synonym</th>
			<th colspan="1">Reference</th>
		</tr>
			{{#each pupa_gene as |pupa_gene|}}
		<tr>
			<td colspan="1"><a href="https://www.ncbi.nlm.nih.gov/gene/{{pupa_gene.id}}" target="_blank">{{pupa_gene.id}}</a></td>
			<td colspan="1">{{pupa_gene.name}}</td>
			<td colspan="1">{{pupa_gene.synonym}}</td>
			<td colspan="1">{{{pupa_gene.url}}}</td>
		</tr>
			{{/each}}
		{{/if}}
	</table>
	{{/if}}

	<!--
		成虫リソース情報
	-->
	{{#if adult}}
	<table class="table table-sm table-striped table-bordered table-adult">
		<thead class="thead-dark">
			<th colspan="4"><h4>Adult</h4></th>
		</thead>
		<tr>
			<th colspan="1" class="sub-title">Resource State</th>
			<td colspan="3">{{adult.resource_state}}</td>
		</tr>
		<tr>
			<th colspan="1" class="sub-title">Image</th>
			<td colspan="3">
		{{#if adult_image}}
			{{#each adult_image as |adult_image|}}
				<img width="120px" src="{{adult_image.image}}">
			{{/each}}
		{{/if}}
			</td>
		</tr>
		{{#if adult_phenotype}}
		<thead class="thead-dark">
			<th colspan=4">Phenotype</th>
		</thead>
		<tr>
			<th colspan="1">Phenotype</th>
			<th colspan="1">BMPO</th>
			<th colspan="1">seeAlso</th>
			<th colspan="1">Reference</th>
		</tr>
			{{#each adult_phenotype as |adult_phenotype|}}
		<tr>
			<td colspan="1">{{{adult_phenotype.label}}}</td>
			<td colspan="1">{{{adult_phenotype.bmpo}}}</td>
			<td colspan="1">{{{adult_phenotype.related_class}}}</td>
			<td colspan="1">{{{adult_phenotype.reference}}}</td>
		</tr>
			{{/each}}
		{{/if}}
		{{#if adult_gene}}
		<thead class="thead-dark">
			<th colspan=4">Gene</th>
		</thead>
		<tr>
			<th colspan="1">Gene ID</th>
			<th colspan="1">Name</th>
			<th colspan="1">Synonym</th>
			<th colspan="1">Reference</th>
		</tr>
			{{#each adult_gene as |adult_gene|}}
		<tr>
			<td colspan="1"><a href="https://www.ncbi.nlm.nih.gov/gene/{{adult_gene.id}}" target="_blank">{{adult_gene.id}}</a></td>
			<td colspan="1">{{adult_gene.name}}</td>
			<td colspan="1">{{adult_gene.synonym}}</td>
			<td colspan="1">{{{adult_gene.url}}}</td>
		</tr>
			{{/each}}
		{{/if}}
	</table>
	{{/if}}

	{{#if information}}
	<div><a href="{{information.homepage}}" target="_blank">Click here to order</a></div>
	{{/if}}
</div>
